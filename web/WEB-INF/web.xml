<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>com.servlets.RegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoctorServlet</servlet-name>
        <servlet-class>com.servlets.DoctorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SetTime</servlet-name>
        <servlet-class>com.servlets.SetTime</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateTime</servlet-name>
        <servlet-class>com.servlets.UpdateTime</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoctorAdd</servlet-name>
        <servlet-class>com.servlets.DoctorAdd</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/RegistrationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoctorServlet</servlet-name>
        <url-pattern>/DoctorServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SetTime</servlet-name>
        <url-pattern>/SetTime</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateTime</servlet-name>
        <url-pattern>/UpdateTime</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoctorAdd</servlet-name>
        <url-pattern>/DoctorAdd</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
